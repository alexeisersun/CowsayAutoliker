{
  "manifest_version" : 2,
  "name": "Our cool autoliker",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js":["jquery.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}